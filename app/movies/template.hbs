{{#each flashMessages.queue as |flash|}}
  {{flash-message flash=flash}}
{{/each}}
<div class="browse-movies col-md-10 col-md-offset-1">
  <h2>Browse Movies</h2>
  {{#each model as |movie|}}
      {{fwf-movie title=movie.title
                  director=movie.director
                  yearReleased=movie.yearReleased
                  genre=movie.genre
                  rating=movie.rating
                  movie=movie
                  createScreening='createScreening'}}
  {{/each}}
</div>
